#include <iostream>
#include<algorithm>
using namespace std;

int main()
{
    int const length = 11;
    int input[11];
    
	cout<<"Enter number: ";
    for(int i = 0; i<11 ; i++){
        
        cin>>input[i];
    }
   
    cout << "Array before sorting: " ;
    cout << "{ ";
    for(int i=0; i<11; i++){
        cout<< input[i]<< " ";
    }
    cout << " }" << endl;
    
	cout << "Array after sorting: " ;
    cout << "[ ";
    sort(input, input+length);
    for(int i=0; i<11; i++){
        cout << input[i]<< " ";
    }
    cout  << " ]" << endl; 
    
    return 0;
}
